<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <TargetFramework>net6.0</TargetFramework>
	<Nullable>enable</Nullable>
    <Authors>Daniele Fusi</Authors>
    <Company>Fusi</Company>
    <Product>Cadmus</Product>
    <Description>Cadmus TGR parts.</Description>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Version>2.1.5</Version>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <Copyright>by Daniele Fusi</Copyright>
    <PackageTags>Cadmus;TGR parts</PackageTags>
    <FileVersion>2.1.5</FileVersion>
    <AssemblyVersion>2.1.5</AssemblyVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Cadmus.Core" Version="4.0.5" />
    <PackageReference Include="Cadmus.General.Parts" Version="2.1.0" />
    <PackageReference Include="Cadmus.Philology.Parts" Version="4.0.0" />
    <PackageReference Include="Cadmus.Refs.Bricks" Version="3.0.1" />
    <PackageReference Include="Cadmus.Seed.General.Parts" Version="2.1.0" />
    <PackageReference Include="Fusi.Antiquity" Version="2.0.0" />
  </ItemGroup>

</Project>
