    <PackageReference Include="Cadmus.Parts" Version="2.7.1" />
    <PackageReference Include="Cadmus.Philology.Parts" Version="2.5.2" />
    <PackageReference Include="Cadmus.Refs.Bricks" Version="2.0.1" />
    <PackageReference Include="Fusi.Antiquity" Version="1.2.0" />
