<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<Nullable>enable</Nullable>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <TargetFramework>net8.0</TargetFramework>
	<Nullable>enable</Nullable>
    <Authors>Daniele Fusi</Authors>
    <Company>Fusi</Company>
    <Product>Cadmus</Product>
    <Description>Cadmus TGR parts.</Description>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Version>7.0.0</Version>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <Copyright>by Daniele Fusi</Copyright>
    <PackageTags>Cadmus;TGR parts</PackageTags>
    <FileVersion>7.0.0</FileVersion>
    <AssemblyVersion>7.0.0</AssemblyVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Cadmus.Core" Version="7.0.0" />
    <PackageReference Include="Cadmus.General.Parts" Version="6.0.1" />
    <PackageReference Include="Cadmus.Philology.Parts" Version="8.0.1" />
    <PackageReference Include="Cadmus.Refs.Bricks" Version="5.0.0" />
    <PackageReference Include="Cadmus.Seed.General.Parts" Version="6.0.1" />
    <PackageReference Include="Fusi.Antiquity" Version="4.0.0" />
  </ItemGroup>

</Project>
